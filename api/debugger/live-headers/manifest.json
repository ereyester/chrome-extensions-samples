{
  "name": "Live HTTP headers",
  "description": "Displays the live log with the http requests headers",
  "version": "0.7.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["debugger"],
  "action": {
    "default_title": "Live HTTP headers",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}